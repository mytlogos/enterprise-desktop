<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="5.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
        <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS" />
        <ChoiceBox fx:id="episodeBox" prefWidth="200.0" />
        <Region HBox.hgrow="ALWAYS" />
        <Button fx:id="previousBtn" mnemonicParsing="false" text="Previous Episode" />
        <Button fx:id="nextBtn" layoutX="248.0" layoutY="15.0" mnemonicParsing="false" text="Next Episode" />
    </HBox>
    <ListView fx:id="list" VBox.vgrow="ALWAYS" />
</VBox>
