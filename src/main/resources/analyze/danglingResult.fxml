<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <HBox alignment="CENTER" spacing="5.0">
        <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0"/>
        <Region HBox.hgrow="ALWAYS"/>
        <Button fx:id="showButton" mnemonicParsing="false" text="Show"/>
    </HBox>
    <HBox spacing="5.0">
        <VBox.margin>
            <Insets/>
        </VBox.margin>
        <ComboBox fx:id="listSelect" editable="true" prefWidth="150.0" promptText="Select List"/>
        <Button fx:id="addButton" mnemonicParsing="false" text="Add To List"/>
    </HBox>
</VBox>
