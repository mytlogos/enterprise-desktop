<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER_RIGHT" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.141"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mytlogos.enterprisedesktop.controller.AnalyzeController">
   <padding>
      <Insets left="5.0" right="5.0" top="5.0"/>
   </padding>
   <Button mnemonicParsing="false" onAction="#analyze" text="Analyze"/>
   <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <TreeView fx:id="problemView" minWidth="200.0" HBox.hgrow="ALWAYS"/>
      <Pane fx:id="displayPane" minWidth="200.0" HBox.hgrow="ALWAYS"/>
   </HBox>
</VBox>
